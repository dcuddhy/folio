{{outlet}}

<center>  <h2>Projects</h2> </center>
<br>

<div class="clearfix">
  <div class="projects">
    {{#each}}
    <div class = "lift-left">
      <h3>{{name}}</h3>
      {{{desc}}}
    </div>
    <div class = "drop-left">
      &nbsp;
    </div>
    <div class = "wrap-left">
    </div>
    {{/each}}
  </div>

  <div class="trainings">
    {{#each training in trainings}}
    <div class = "lift-right">
      <h3>{{title}}</h3>
      {{{text}}}
    </div>
    <div class = "drop-right">
      &nbsp;
    </div>
    <div class = "wrap-right">
    </div>
    {{/each}}
  </div>
</div>
