Portfolio
<div class="clearfix">
  <div class="projects">
    {{#each}}
    <div class = "lift-left">
            <span class = "thumb">{{{thumb}}}</span>
      <h3>{{name}}</h3>
      {{{desc}}}
    </div>
    <div class = "drop-left">
      &nbsp;
    </div>
    <div class = "wrap-left">
    </div>
    {{/each}}
  </div>

  <div class="trainings">
    <!-- in ruby this is `trainings.each do |training|` -->
    {{#each training in trainings}}
    <div class = "lift-right">
      <h3>{{training.name}}</h3>
      {{{training.desc}}}<br>
      {{{training.date}}}
    </div>
    <div class = "drop-right">
      &nbsp;
    </div>
    <div class = "wrap-right">
    </div>
    {{/each}}
  </div>
</div>

